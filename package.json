{
  "name": "prototype-starter-kit",
  "version": "1.0.0",
  "description": "a starter kit with SASS, Gulp and Budo for quick prototyping",
  "main": "index.js",
  "scripts": {
    "start": "gulp watch",
    "open": "gulp watch --open",
    "bundle": "gulp bundle --production",
    "deploy": "gulp bundle && git subtree push --prefix app origin gh-pages"
  },
  "keywords": [
    "html",
    "css",
    "sass",
    "budo"
  ],
  "author": "Patrick Gerritsen",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "budo": "^8.1.0",
    "csswring": "^4.2.2",
    "gulp": "^3.9.1",
    "gulp-if": "^2.0.0",
    "gulp-postcss": "^6.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^1.5.3",
    "gulp-watch": "^4.3.5",
    "postcss-assets": "^4.1.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^4.4.0"
  }
}
