{
  "name": "prototype-starter-kit",
  "version": "1.1.0",
  "description": "a starter kit with SASS, Gulp and Budo for quick prototyping",
  "main": "index.js",
  "scripts": {
    "start": "gulp serve",
    "open": "npm run start -- --open",
    "bundle": "gulp bundle --production",
    "deploy": "gulp bundle && git subtree push --prefix site origin gh-pages",
    "test": "zuul -- source/**/*.test.js"
  },
  "keywords": [
    "html",
    "css",
    "sass",
    "budo"
  ],
  "author": "Patrick Gerritsen",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.5.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "*",
    "babelify": "*",
    "browser-sync": "^2.18.5",
    "browserify": "^13.0.1",
    "cssnano": "^3.9.1",
    "envify": "^4.0.0",
    "event-stream": "^3.3.4",
    "gulp": "*",
    "gulp-if": "^2.0.2",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "*",
    "gulp-rename": "*",
    "gulp-sass": "^3.0.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-uglify": "*",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.11",
    "gulp-yaml": "^1.0.1",
    "loose-envify": "^1.3.0",
    "marked": "^0.3.6",
    "normalize.css": "^5.0.0",
    "postcss-assets": "*",
    "postcss-import": "^9.0.0",
    "strip-ansi": "^3.0.1",
    "tape": "^4.6.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "*",
    "watchify": "^3.8.0",
    "yargs": "^6.5.0",
    "zuul": "^3.11.1"
  }
}
