{
  "name": "prototype-starter-kit",
  "version": "1.0.0",
  "description": "a starter kit with SASS, Gulp and Budo for quick prototyping",
  "main": "index.js",
  "scripts": {
    "start": "gulp watch",
    "open": "gulp watch --open",
    "bundle": "gulp bundle --production",
    "deploy": "gulp bundle && git subtree push --prefix app origin gh-pages"
  },
  "keywords": [
    "html",
    "css",
    "sass",
    "budo"
  ],
  "author": "Patrick Gerritsen",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer"       : "*",
    "babel-preset-es2015": "*",
    "babel-preset-react" : "*",
    "babelify"           : "*",
    "budo"               : "*",
    "csswring"           : "*",
    "gulp"               : "*",
    "gulp-if"            : "*",
    "gulp-postcss"       : "*",
    "gulp-rename"        : "*",
    "gulp-sass"          : "*",
    "gulp-streamify"     : "*",
    "gulp-uglify"        : "*",
    "gulp-watch"         : "*",
    "postcss-assets"     : "*",
    "vinyl-source-stream": "*",
    "yargs"              : "*"
  }
}
